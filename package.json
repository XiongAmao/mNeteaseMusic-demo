{
  "name": "Netease-music",
  "version": "1.0.0",
  "description": "",
  "main": "homepage.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset": "rm -rf ./build && mkdir build && mkdir build/js && mkdir build/css",
    "cp": "cp -r homepage.html  song.html  ./json ./images ./build ",
    "css": " node_modules/postcss-cli/bin/postcss ./css/*.scss --use autoprefixer -d build/css",
    "sass": "node_modules/node-sass/bin/node-sass ./build/css/song.scss --output-style compressed ./build/css/song.css && node_modules/node-sass/bin/node-sass ./build/css/homepage.scss --output-style compressed ./build/css/homepage.css ",
    "babel": "node_modules/babel-cli/bin/babel.js ./js/song.js --out-file ./build/js/song.js && node_modules/babel-cli/bin/babel.js ./js/homepage.js --out-file ./build/js/homepage.js",
    "uglifyjs": "node_modules/uglify-js/bin/uglifyjs  ./build/js/song.js -o ./build/js/song.js && node_modules/uglify-js/bin/uglifyjs ./build/js/homepage.js -o ./build/js/homepage.js",
    "build": "npm run reset && npm run cp && npm run css && npm run sass && npm run babel && npm run uglifyjs",
    "gulp":"./node_modules/gulp/bin/gulp.js",
    "start": "cd build ; ../node_modules/node-static/bin/cli.js -i "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^3.5.0",
    "gulp-concat": "^2.6.1",
    "gulp-html-minify": "0.0.14",
    "gulp-postcss": "^7.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.9",
    "postcss-cli": "^4.1.0",
    "uglify-js": "^3.0.23"
  }
}
