{
  "name": "Netease-music",
  "version": "1.0.0",
  "description": "",
  "main": "homepage.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset": "rm -rf ./build1 && mkdir build1 && mkdir build1/js && mkdir build1/css",
    "cp": "cp -r homepage.html  song.html  ./json ./images ./build1 ",
    "css": " node_modules/postcss-cli/bin/postcss ./css/*.scss --use autoprefixer -d build1/css",
    "sass": "node_modules/node-sass/bin/node-sass ./build1/css/song.scss --output-style compressed ./build1/css/song.css && node_modules/node-sass/bin/node-sass ./build1/css/homepage.scss --output-style compressed ./build1/css/homepage.css ",
    "babel": "node_modules/babel-cli/bin/babel.js ./js/song.js --out-file ./build1/js/song.js && node_modules/babel-cli/bin/babel.js ./js/homepage.js --out-file ./build1/js/homepage.js",
    "uglifyjs": "node_modules/uglify-js/bin/uglifyjs  ./build1/js/song.js -o ./build1/js/song.js && node_modules/uglify-js/bin/uglifyjs ./build1/js/homepage.js -o ./build1/js/homepage.js",
    "build1": "npm run reset && npm run cp && npm run css && npm run sass && npm run babel && npm run uglifyjs",
    "start": "node_modules/node-static/bin/cli.js -i build1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.9",
    "postcss-cli": "^4.1.0",
    "uglify-js": "^3.0.23"
  }
}
